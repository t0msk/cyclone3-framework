	$XSGN{LINE} = <<"TMP_line";
<tr class="tableinfos">
	<td><%domain%></td>
	<td><%subdomain%></td>
	<td><%type%></td>
	<td><%count%></td>
	<td><%load_req%></td>
	<td><%load_proc%></td>
	<td><%fulltime1%>s <%fulltime2%>%</td>

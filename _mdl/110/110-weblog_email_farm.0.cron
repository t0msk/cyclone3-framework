		<td class="summary-item">&nbsp;</td>
	</tr>
	<#LINE_performbus#>
	<#LINE_performload#>
	<#LINE_performproc#>
</table>
PART

	$XSGN{LINE_domain} = <<"TMP_line_domain";
<tr>
	<td colspan="8">&nbsp;</td>
</tr>
<tr class="tableinfos">
	<th colspan="8"><%domain-name%></th>
</tr>
<tr class="tableinfos">
	<th><%domain%></th>
	<th><%subdomain%></th>
	<th><%type%></th>

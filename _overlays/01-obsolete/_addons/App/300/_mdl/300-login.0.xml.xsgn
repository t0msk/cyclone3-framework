	<reason><![CDATA[No login or password entered]]></reason>
</response>
</DEFINITION>

<DEFINITION id="err_badpassoruser">
<response IDapp="a300" type="login" result="0" code="<$main::request_code>">
	<reason><![CDATA[Login incorrect]]></reason>
</response>
</DEFINITION>

<DEFINITION id="err_logged">
<response IDapp="a300" type="login" result="0" code="<$main::request_code>">
	<reason><![CDATA[You are already logged on from another location]]></reason>
</response>
</DEFINITION>

</XML_DESIGN_DEFINITION>

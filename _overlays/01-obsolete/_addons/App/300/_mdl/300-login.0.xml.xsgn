<XML_DESIGN_DEFINITION>

<DEFINITION id="logged">
<response IDapp="a300" type="login" result="1" code="<$main::request_code>">
	<![CDATA[You have been successfully signed in]]>
</response>
</DEFINITION>

<DEFINITION id="err_cookies">
<response IDapp="a300" type="login" result="0" code="<$main::request_code>">
	<reason><![CDATA[Unable to login, because your cookies are dissabled!!!]]></reason>
</response>
</DEFINITION>

<DEFINITION id="err_missed">
